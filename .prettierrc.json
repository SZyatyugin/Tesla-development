{
    "printWidth": 80,
    "tabWidth": 4,
    "singleQuote": false,
    "trailingComma":"es5",
    "bracketSpacing": true,
    "semi": true,
    "useTabs": false,
    "jsxBracketSameLine": false,
    "overrides": [
       {
           "files":"*.js",
            "options": {
                "parser":"babel"
            }
        },
        {
            "files":"*.html",
            "options":{
              "parser":"html"
            }
          }
    ]
}